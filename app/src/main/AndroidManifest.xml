<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.project207">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hospital"
        android:label="동네병원"
        android:roundIcon="@mipmap/hospital"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.RegisterActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.ReserveActivity" />
        <activity android:name=".Activity.MyinfoActivity" />
        <activity android:name=".Activity.CheckbookActivity" />
        <activity android:name=".Activity.HistoryActivity" />
        <activity
            android:name=".Activity.SplashActivity"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".Activity.SetDateActivity" />
        <activity android:name=".Activity.CheckWaitingActivity"></activity>
        <activity android:name=".Activity.Pop"
            android:theme="@style/AppTheme.pop"></activity>

    </application>

</manifest>